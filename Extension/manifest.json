{
  "name":           "JMX Creator",
  "version":        "0.1",
  "description":    "Create .jmx file ",
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions":    [
    "webNavigation", "storage","webRequest","https://*/*","http://*/*"
  ],
  "manifest_version": 3
}
