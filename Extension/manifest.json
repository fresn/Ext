{
  "name": "JMX Creator",
  "version": "0.1",
  "description": "Create .jmx file ",
  "background": {
    "page": "background.html",
    "default_popup": "",
    "persistent": true
    //    "scripts": ["background.js","taffy.js"]
  },
  "browser_action": {
//    "default_popup": "main.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "storage",
    "webRequest",
    "https://*/",
    "http://*/",
    "http://www.google.com/*",
    "https://www.google.com/*",
//    "https://www.google.ca/*",
    "notifications",
    "browsingData"
  ],
  "manifest_version": 2
}
