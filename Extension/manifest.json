{
  "name": "JMX Creator",
  "version": "0.1",
  "description": "Create .jmx file ",
  "background": {
    "page": "background.html",
    "default_popup": "",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "storage",
    "webRequest",
    "https://*/",
    "http://*/",
    "notifications",
    "browsingData",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.google.com/*",
      "*://*.google.ca/*"
    ]
  },
  "manifest_version": 2
}
